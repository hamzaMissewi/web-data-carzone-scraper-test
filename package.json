{
  "name": "carzone-crawler-node",
  "version": "1.0.0",
  "description": "CarZone.ie web crawler built with Node.js, TypeScript, Axios, and Cheerio",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index2.ts",
    "dev:2": "ts-node src/index.ts",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "test": "ts-node src/tests/run-all.test.ts",
    "docker:build": "docker build -t carzone-crawler .",
    "docker:run": "docker run --rm -v \"${PWD}/html:/app/html\" carzone-crawler",
    "docker:proxy": "docker run --rm -v \"${PWD}/html:/app/html\" -e PROXY_URL=\"$PROXY_URL\" carzone-crawler",
    "test:connection": "ts-node src/debug_check.ts",
    "test:crawler": "ts-node src/tests/crawler.test.ts",
    "test:utils": "ts-node src/tests/utils.test.ts"
  },
  "keywords": [
    "crawler",
    "web-scraping",
    "carzone"
  ],
  "author": "Hamza Missaoui",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.2",
    "playwright": "^1.57.0",
    "socks-proxy-agent": "^8.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
